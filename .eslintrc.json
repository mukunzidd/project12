{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
    "indent": "off",
    "block-spacing": "off",
    "no-irregular-whitespace": 0,
    "no-console": 0,
    "no-empty": 0,
    "no-trailing-spaces": "off",
    "comma-spacing": "off",
    "object-curly-spacing": "off",
    "object-curly-newline": "off",
    "comma-dangle": "off",
    "key-spacing": "off",
    "arrow-body-style": "off",
    "space-infix-ops": "off",
    "brace-style": "off",
    "max-len": "off",
    "arrow-parens": "off",
    "prefer-spread": "off",
    "prefer-template": "off",
    "no-undef": "off",
    "radix": "off",
    "consistent-return":"off",
    "newline-per-chained-call": "off",
    "prefer-promise-reject-errors": "off",
    "no-unused-expressions": "off"
    }
}